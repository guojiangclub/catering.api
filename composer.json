{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.0.0",
		"anam/phantomjs-linux-x86-binary": "^2.1",
		"anam/phantommagick": "dev-docker",
		"arrilot/laravel-widgets": "^3.7",
		"barryvdh/laravel-cors": "^0.10",
		"dingo/api": "2.0.0-alpha1",
		"doctrine/dbal": "^2.8",
		"fideloper/proxy": "~3.3",
		"ibrand/backend": "dev-master",
		"ibrand/laravel-active": "~2.0",
		"ibrand/laravel-aliyun-mail": "^1.1",
		"ibrand/laravel-cache": "~1.0",
		"ibrand/laravel-database-logger": "~1.0",
		"ibrand/laravel-miniprogram-poster": "^1.1",
		"ibrand/laravel-shopping-cart": "^1.3",
		"ibrand/laravel-sms": "~1.0",
		"ibrand/laravel-ueditor": "^1.0",
		"ibrand/setting": "~1.0",
		"ibrand/setting-backend": "~1.0",
		"ibrand/wechat-backend": "dev-master",
		"intervention/image": "^2.3",
		"jenssegers/agent": "^2.4",
		"kalnoy/nestedset": "^4.1",
		"laracasts/flash": "^2.0",
		"laravel/framework": "5.5.*",
		"laravel/homestead": "^6.6",
		"laravel/passport": "^4.0",
		"laravel/tinker": "~1.0",
		"laravelcollective/html": "^5.5",
		"lavary/laravel-menu": "~1.7",
		"league/fractal": "^0.17.0",
		"maatwebsite/excel": "~2.1.23",
		"marktopper/doctrine-dbal-timestamp-type": "^1.0",
		"milon/barcode": "^5.3",
		"overtrue/laravel-ueditor": "~1.0",
		"overtrue/laravel-uploader": "~0.0.7",
		"overtrue/laravel-wechat": "~4.0",
		"pingplusplus/pingpp-php": "^2.1",
		"predis/predis": "1.*",
		"prettus/l5-repository": "^2.1",
		"qiniu/php-sdk": "^7.0",
		"rtconner/laravel-tagging": "^2.2",
		"sentry/sentry-laravel": "~0.1",
		"simplesoftwareio/simple-qrcode": "~2",
		"wikimedia/composer-merge-plugin": "^1.3",
		"yajra/laravel-datatables-oracle": "~6.0",
		"yansongda/pay": "^2.0",
		"zizaco/entrust": "5.2.x-dev"
	},
	"require-dev": {
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0",
		"symfony/thanks": "^1.0",
		"barryvdh/laravel-debugbar": "~3.0",
		"barryvdh/laravel-ide-helper": "^2.4"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"merge-plugin": {
			"include": [
				"modules/*/composer.json",
				"modules/*/*/composer.json"
			],
			"recurse": true,
			"replace": false,
			"merge-dev": true,
			"merge-extra": false,
			"merge-extra-deep": false
		},
		"laravel": {
			"dont-discover": [
			]
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"repositories": [
		{
			"packagist": false
		},
		{
			"type": "composer",
			"url": "https://mirrors.aliyun.com/composer/"
		}
	]
}
